{
  "name": "ngx-fileupload",
  "version": "11.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --configuration e2e --port=4201",
    "e2e:watch": "watch 'ng e2e --port=4201' lib example e2e --wait=1",
    "build:demo": "ng build --configuration=demo --base-href=/ngx-fileupload/",
    "deploy:demo": "npx ngh --dir=dist\\ngx-fileupload-example",
    "build:core": "ng build core",
    "package:core": "tar -cvf ./dist/core.tar.gz -C dist/core .",
    "package:ui": "tar -cvf ./dist/ui.tar.gz -C dist/ui .",
    "install:core": "npm i --save ./dist/core.tar.gz",
    "github-actions:unit-tests-core": "ng test core --watch=false --code-coverage",
    "github-actions:unit-tests-ui": "ng test ui --watch=false --code-coverage",
    "github-actions:e2e-chrome": "ng e2e --configuration=github-actions-chrome --port=4201",
    "github-actions:e2e-firefox": "ng e2e --configuration=github-actions-firefox --port=4201",
    "test:code-coverage": "ng test --code-coverage",
    "webdriver:update": "webdriver-manager update --version.gecko=v0.26.0 --versions.chrome 78.0.3904.70 --standalone false"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "17.3.4",
    "@angular/animations": "17.3.4",
    "@angular/cdk": "17.3.4",
    "@angular/common": "17.3.4",
    "@angular/compiler": "17.3.4",
    "@angular/core": "17.3.4",
    "@angular/forms": "17.3.4",
    "@angular/platform-browser": "17.3.4",
    "@angular/platform-browser-dynamic": "17.3.4",
    "@angular/router": "17.3.4",
    "highlight.js": "11.4.0",
    "igniteui-theming": "6.0.2",
    "ngx-dropzone": "3.0.0",
    "ngx-file-drop": "13.0.0",
    "ngx-highlightjs": "6.1.3",
    "rxjs": "7.4.0",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.4",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.4",
    "@angular/compiler-cli": "17.3.4",
    "@angular/language-service": "17.3.4",
    "@types/jasmine": "3.10.5",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "20.5.6",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "bootstrap-scss": "5.1.3",
    "codelyzer": "6.0.2",
    "cors": "2.8.5",
    "eslint": "8.57.0",
    "express": "4.17.3",
    "express-fileupload": "1.2.1",
    "https-proxy-agent": "5.0.0",
    "igniteui-angular": "17.1.8",
    "jasmine-core": "4.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.1.0",
