{
  "name": "ngx-fileupload",
  "version": "7.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "compodoc": "npx compodoc -p lib/tsconfig.lib.json --disablePrivate --disableLifeCycleHooks",
    "e2e": "ng e2e --configuration e2e --port=4201",
    "e2e:watch": "watch 'ng e2e --port=4201' lib example e2e --wait=1",
    "build:demo": "ng build --configuration=demo --base-href=/ngx-fileupload/",
    "deploy:demo": "npx ngh --dir=dist\\ngx-fileupload-example",
    "build:core": "ng build core",
    "package:core": "tar -cvf ./dist/core.tar.gz -C dist/core .",
    "package:ui": "tar -cvf ./dist/ui.tar.gz -C dist/ui .",
    "install:core": "npm i --save ./dist/core.tar.gz",
    "github-actions:unit-tests-core": "ng test core --watch=false --code-coverage",
    "github-actions:unit-tests-ui": "ng test ui --watch=false --code-coverage",
    "github-actions:e2e-chrome": "ng e2e --configuration=github-actions-chrome --port=4201",
    "github-actions:e2e-firefox": "ng e2e --configuration=github-actions-firefox --port=4201",
    "test:code-coverage": "ng test --code-coverage",
    "webdriver:update": "webdriver-manager update --version.gecko=v0.26.0 --versions.chrome 78.0.3904.70 --standalone false"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "12.2.7",
    "@angular/animations": "12.2.7",
    "@angular/cdk": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/router": "12.2.7",
    "highlight.js": "11.2.0",
    "ngx-dropzone": "3.0.0",
    "ngx-file-drop": "11.2.0",
    "ngx-highlightjs": "5.0.0",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "web-animations-js": "2.3.2",
    "igniteui-angular": "12.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@angular/language-service": "12.2.7",
    "@compodoc/compodoc": "1.1.15",
    "@types/jasmine": "3.9.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "14.17.18",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "angular-cli-ghpages": "0.6.2",
    "bootstrap-scss": "5.1.1",
    "codelyzer": "6.0.2",
    "cors": "2.8.5",
    "eslint": "7.32.0",
    "express": "4.17.1",
    "express-fileupload": "1.2.1",
    "https-proxy-agent": "5.0.0",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "karma-mocha-reporter": "2.2.5",
    "letslog": "1.0.12",
    "ng-packagr": "12.2.2",
    "protractor": "7.0.0",
    "ts-node": "10.2.1",
    "tsickle": "0.43.0",
    "tslint": "6.1.3",
    "typescript": "^4.2.3",
    "watch": "1.0.2",
