{
  "name": "ngx-fileupload",
  "version": "9.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --configuration e2e --port=4201",
    "e2e:watch": "watch 'ng e2e --port=4201' lib example e2e --wait=1",
    "build:demo": "ng build --configuration=demo --base-href=/ngx-fileupload/",
    "deploy:demo": "npx ngh --dir=dist\\ngx-fileupload-example",
    "build:core": "ng build core",
    "package:core": "tar -cvf ./dist/core.tar.gz -C dist/core .",
    "package:ui": "tar -cvf ./dist/ui.tar.gz -C dist/ui .",
    "install:core": "npm i --save ./dist/core.tar.gz",
    "github-actions:unit-tests-core": "ng test core --watch=false --code-coverage",
    "github-actions:unit-tests-ui": "ng test ui --watch=false --code-coverage",
    "github-actions:e2e-chrome": "ng e2e --configuration=github-actions-chrome --port=4201",
    "github-actions:e2e-firefox": "ng e2e --configuration=github-actions-firefox --port=4201",
    "test:code-coverage": "ng test --code-coverage",
    "webdriver:update": "webdriver-manager update --version.gecko=v0.26.0 --versions.chrome 78.0.3904.70 --standalone false"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "14.0.3",
    "@angular/animations": "14.0.3",
    "@angular/cdk": "14.0.3",
    "@angular/common": "14.0.3",
    "@angular/compiler": "14.0.3",
    "@angular/core": "14.0.3",
    "@angular/forms": "14.0.3",
    "@angular/platform-browser": "14.0.3",
    "@angular/platform-browser-dynamic": "14.0.3",
    "@angular/router": "14.0.3",
    "highlight.js": "11.5.1",
    "ngx-dropzone": "3.1.0",
    "ngx-file-drop": "14.0.1",
    "ngx-highlightjs": "7.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.3",
    "@angular-eslint/builder": "14.0.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/schematics": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "14.0.3",
    "@angular/compiler-cli": "14.0.3",
    "@angular/language-service": "14.0.3",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.41",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "angular-cli-ghpages": "1.0.0",
    "bootstrap-scss": "5.1.3",
    "codelyzer": "6.0.2",
    "cors": "2.8.5",
    "eslint": "8.18.0",
    "express": "4.18.1",
    "express-fileupload": "1.4.0",
    "https-proxy-agent": "5.0.1",
    "igniteui-angular": "14.0.2",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
